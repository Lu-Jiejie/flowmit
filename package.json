{
  "name": "flowmit",
  "type": "module",
  "version": "1.0.10",
  "packageManager": "pnpm@10.28.0",
  "description": "Commit like the flow in Git.",
  "author": "Lu-Jiejie <lu-jiejie@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/Lu-Jiejie/flowmit#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lu-Jiejie/flowmit.git"
  },
  "bugs": "https://github.com/Lu-Jiejie/flowmit/issues",
  "keywords": [
    "git",
    "commit",
    "commit message",
    "cli"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./cli": "./dist/cli.mjs",
    "./package.json": "./package.json"
  },
  "bin": {
    "fm": "./bin/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown",
    "lint": "eslint .",
    "release": "bumpp",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build",
    "prepare": "simple-git-hooks",
    "cli": "tsx src/cli.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.0.1",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.19.29",
    "bumpp": "^10.4.0",
    "changelogithub": "^14.0.0",
    "eslint": "^9.39.2",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.7",
    "minimist": "^1.2.8",
    "picocolors": "^1.1.1",
    "prompts-plus": "^3.0.4",
    "publint": "^0.3.16",
    "simple-git-hooks": "^2.13.1",
    "tsdown": "0.20.0-beta.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.17"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
